<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.tizen.example.Accounts" version="1.0.0" api-version="4" xmlns="http://tizen.org/ns/packages">
    <profile name="wearable" />
    <ui-application appid="org.tizen.example.Accounts" exec="Accounts.dll" multiple="false" nodisplay="false" taskmanage="true" splash-screen-display="true" type="dotnet" launch_mode="single">
        <label>Accounts</label>
        <icon>Accounts.png</icon>
        <metadata key="http://tizen.org/metadata/prefer_dotnet_aot" value="true" />
        <splash-screens />
    </ui-application>
    <shortcut-list />
    <account>
        <account-provider appid="org.tizen.example.Accounts" multiple-accounts-support="true" providerid="org.tizen.example.Accounts">
            <icon section="account">Accounts.png</icon>
            <icon section="account-small">Accounts.png</icon>
            <label>Accounts sample app</label>
        </account-provider>
    </account>
    <privileges>
        <privilege>http://tizen.org/privilege/account.read</privilege>
        <privilege>http://tizen.org/privilege/account.write</privilege>
    </privileges>
    <provides-appdefined-privileges />
    <feature name="http://tizen.org/feature/account">true</feature>
</manifest>
