<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="9.0" package="org.tizen.example.Downloader" version="1.0.0">
  <profile name="common" />
  <privileges>
    <privilege>http://tizen.org/privilege/download</privilege>
  </privileges>
  <ui-application appid="org.tizen.example.Downloader"
					exec="Downloader.dll"
					type="dotnet-nui"
					multiple="false"
					taskmanage="true"
					nodisplay="false"
					launch_mode="single"
                    api-version="12">
    <label>Downloader</label>
    <icon>Downloader.png</icon>
    <metadata key="http://tizen.org/metadata/prefer_dotnet_aot" value="true" />
  </ui-application>
</manifest>
