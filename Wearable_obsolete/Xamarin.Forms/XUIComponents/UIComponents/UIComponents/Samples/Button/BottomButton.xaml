<?xml version="1.0" encoding="utf-8" ?>
<w:CirclePage
    x:Class="UIComponents.Samples.Button.BottomButton"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:UIComponents.Samples.Button"
    xmlns:sys="clr-namespace:System;assembly=netstandard"
    xmlns:w="clr-namespace:Tizen.Wearable.CircularUI.Forms;assembly=Tizen.Wearable.CircularUI.Forms">
    <w:CirclePage.Content>
        <StackLayout Padding="0,30" Orientation="Vertical">
            <Label
                FontAttributes="Bold"
                FontSize="Medium"
                HorizontalOptions="Center"
                Text="Bottom"
                TextColor="#0FB4EF" />
        </StackLayout>
    </w:CirclePage.Content>
    <w:CirclePage.ActionButton>
        <w:ActionButtonItem Text="BOTTOM" />
    </w:CirclePage.ActionButton>
</w:CirclePage>