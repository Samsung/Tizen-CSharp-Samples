<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:e="clr-namespace:ElottieSharp.Forms;assembly=ElottieSharp.Forms"
             x:Class="Workout.Views.Workout.CompletedPageView"
             NavigationPage.HasNavigationBar="False">

    <ContentPage.Content>
        <AbsoluteLayout>
            <Image Source="images/workout_complete_bg.png" />
            <e:ElottieAnimationView AbsoluteLayout.LayoutFlags="All"
                                    AbsoluteLayout.LayoutBounds=".5, .35, .23, .23"
                                    AnimationFile="animations/completed.json"
                                    AutoPlay="True"
                                    AutoRepeat="False" />
        </AbsoluteLayout>
    </ContentPage.Content>

</ContentPage>
