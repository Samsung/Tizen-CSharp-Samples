<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="10.0" package="org.tizen.example.StorageSample" version="1.0.0">
  <profile name="common" />
  <service-application appid="org.tizen.example.StorageSample"
					exec="StorageSample.dll"
					type="dotnet"
					multiple="false"
					taskmanage="false"
					nodisplay="true"
                    api-version="13">
    <label>StorageSample</label>
    <icon>StorageSample.png</icon>
  </service-application>
  
  <!-- Storage API Privileges -->
  <privileges>
    <privilege>http://tizen.org/privilege/mediastorage</privilege>
    <privilege>http://tizen.org/privilege/externalstorage</privilege>
    <privilege>http://tizen.org/privilege/systemsettings</privilege>
  </privileges>
</manifest>
