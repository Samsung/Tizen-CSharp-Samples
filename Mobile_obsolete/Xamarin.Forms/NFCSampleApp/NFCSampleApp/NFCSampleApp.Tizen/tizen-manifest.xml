<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="4" package="org.tizen.example.NFCSampleApp.Tizen" version="1.0.0">
	<profile name="common" />
	<ui-application appid="org.tizen.example.NFCSampleApp.Tizen"
					exec="NFCSampleApp.Tizen.dll"
					type="dotnet"
					multiple="false"
					taskmanage="true"
					nodisplay="false"
					launch_mode="single">
  <label>NFCSampleApp.Tizen</label>
	<icon>NFCSampleApp.Tizen.png</icon>
